{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "distDir": "dist"  
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dist/server.js"  
        }
    ]
}
  